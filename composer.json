{
    "name": "cjtterabytesoft/alert",
    "description": "Widget Alert. [Yii 3.0/Bootstrap4]",
    "keywords": ["yii3", "bootstrap4", "widget", "alert"],
    "homepage": "https://github.com/cjtterabytesoft/alert",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/cjtterabytesoft/alert/issues",
        "source": "https://github.com/cjtterabytesoft/alert/widgets",
        "forum":  "https://discuss.tusoporte.net"
    },
    "authors": [
        {
            "name": "Wilmer Arambula",
            "email": "terabytefrelance@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii-core": "^3.0@dev",
        "yiisoft/yii-bootstrap4": "^3.0@dev"
    },
    "require-dev": {
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev",
        "phpunit/phpunit": "^7.3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {"cjtterabytesoft\\widgets\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"cjtterabytesoft\\widgets\\tests\\": "tests"}
    },
    "config": {
        "process-timeout": 1800
    }
}
